<h1>新規投稿<h1>
<%= form_for(@blog) do |f|%>

	<h4>title<h4>
	<%= f.text_field :title %>

	<h4>本文<h4>
	<%= f.text_area :body %>

	<h4>category<h4>
	<%= f.text_field :category %>

	<h4>画像</h4>
	<%= f.attachment_field :blog_image %>

	<%= f.submit %>


<% end %>
